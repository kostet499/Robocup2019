prefix=/usr
exec_prefix=/usr
libdir=/usr/lib
includedir=/usr/include

plugindir=${libdir}/redland

swig=${prefix}/share/redland/Redland.i

have_raptor2_api=1

Name: Redland RDF API Library
Description: Redland RDF API and triple store library
Requires.private: raptor2 >= 2.0.7, rasqal >= 0.9.25, rasqal <= 0.9.99
Version: 1.0.17
Libs: -L${libdir} -lrdf
Libs.private: -Wl,-O1 -Wl,--hash-style=gnu -Wl,--as-needed -Wl,-O1 -Wl,--hash-style=gnu -Wl,--as-needed -ldb-6.0
Cflags: -I${includedir}
